enum VehicleType:byte {Pedestrian, Cyclist, Car}

table Vehicle {
    vehicleType:VehicleType;
    velocity:int;
}

table Client {
    name:string;
    id:int;
    ipAddress:string;
    port:int;
    vehicle:Vehicle;
}

table Team {
    clients:[Client];
}

table Game {
  id:int;
  teams:[Team];
  city:string;
  host:int;
  clientsMax:int;
}

root_type Game;