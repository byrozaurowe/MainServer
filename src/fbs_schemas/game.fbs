enum VehicleType:byte {Pedestrian, Cyclist, Car}

table Clients {
    name:string;
    ipAddress:string;
    socket:int;
}

table Vehicle {
    vehicleType:VehicleType;
    velocity:int;
    free:bool;
}

table Team {
    clients:[Client];
    vehicles:[Vehicle];
}

table Game {
  id:int;
  teams:[Team];
  city:string;
  host:string;
}

root_type Game;